            let soLuongLuaChonPhongElements = document.querySelectorAll('.rooms-list .room-option-quantity');
            let giaTienLuaChonPhongElements = document.querySelectorAll('.rooms-list .room-option-current-price');
            let totalPriceElement = document.querySelector('.total-price');
            let numberOfNights = document.querySelector('#nights').value;
            let numberOfAdults = document.querySelector('#number-of-adults').value;
            let numberOfChilders = document.querySelector('#number-of-childrens').value;
            let numberOfRooms = document.querySelector('#number-of-rooms').value;
            let sleepsAdultElements = document.querySelectorAll('.rooms-list .sleeps .sleeps__adult');
            let sleepsChildrenElemeents = document.querySelectorAll('.rooms-list .sleeps .sleeps__children');

            function handleChangeQuantity() {
                let html = '';
                let totalPrice = 0;
                let totalAdults = 0;
                let totalChildrens = 0;
                let priceDetail = '';
                let totalRooms = 0;
                for (let i = 0; i < soLuongLuaChonPhongElements.length; i++) {
                    totalPrice += +soLuongLuaChonPhongElements[i].value * +giaTienLuaChonPhongElements[i].value;
                    // console.log(+soLuongLuaChonPhongElements[i].value, +giaTienLuaChonPhongElements[i].value)

                    if (+soLuongLuaChonPhongElements[i].value) {
                        totalRooms += +soLuongLuaChonPhongElements[i].value;
                        totalAdults += +soLuongLuaChonPhongElements[i].value * +sleepsAdultElements[i].value;
                        totalChildrens += +soLuongLuaChonPhongElements[i].value * +sleepsChildrenElemeents[i].value;
                        priceDetail += '<span class="price-detail">' + +soLuongLuaChonPhongElements[i].value + ' x ' + xulyTien(giaTienLuaChonPhongElements[i].value) + ' VND/phòng</span><br>'

                        // roomOptionElements_cd1[i].classList[1]: LCP1, LCP2, ...
                        // hidden input for idLCP
                        priceDetail += `<input hidden type="text" name="${roomOptionElements_cd1[i].classList[1]}" id="" value="${+soLuongLuaChonPhongElements[i].value}">`
                    }
                }
                html += '<div><span> Giá tiền cho 1 đêm </span><br>' + priceDetail + '</div>';
                html += "<b>Tổng: " + xulyTien(totalPrice) + " VND</b>";

                // hidden input for thành tiền
                html += `<input hidden type="text" name="totalPrice" id="" value="${totalPrice * numberOfNights}">`
                // console.log(totalPrice);

                let message = '';
                if (totalAdults < numberOfAdults) {
                    message += '<b><span style="color: red;">' + (numberOfAdults - totalAdults) + ' người lớn chưa có phòng </span></b><br>';
                }
                if (totalChildrens < numberOfChilders) {
                    message += '<b><span style="color: red;">' + (numberOfChilders - totalChildrens) + ' trẻ em chưa có phòng </span></b><br>';
                }
                let bookBtn = '';
                if (!message) {
                    bookBtn = '<button type="submit" class="btn btn-primary">Đặt những phòng này</button>';
                    message = '<b><p style="color: green;">Bạn đã chọn đủ phòng cho mọi người!</p></b>';
                }
                message = '<div>' + message + '</div>'
                let totalRoomsMessage = `<p>Bạn đã chọn:<span style="color: blue"><b> ${totalRooms} </b></span> phòng </p>`;
                if (totalRooms == numberOfRooms) totalRoomsMessage = `<p>Bạn đã chọn:<span style="color: green"><b> ${totalRooms} </b></span> phòng <span style="color: green"><b> (Giống với yêu cầu) </b> </span> </p>`;

                let sleepsMessage = `<div>${totalRoomsMessage}<p>Sức chứa tối đa: <i class="fa-solid fa-user"></i> x ${totalAdults}, <i class="fa-solid fa-child-reaching"></i> x ${totalChildrens}</p></div>`
                html += `<div><h5><b>Thành tiền cho ${numberOfNights} đêm: ` + '<br />' + xulyTien(totalPrice * numberOfNights) + " VND</b></h5>" + bookBtn + "</div>"
                html = message + sleepsMessage + html;
                totalPriceElement.innerHTML = html;
            }
            for (let item of soLuongLuaChonPhongElements) {
                // item.oninput = handleChangeQuantity;
                item.onchange = handleChangeQuantity;
            }
        </script>
