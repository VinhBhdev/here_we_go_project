                                                        <div class="content">
                                                            <!-- dấu tên loại phòng tạm vào đây -->
                                                            <input hidden class="type-room-name" type="text"
                                                                value="<%= Object.values(loaiPhong)[0].tenLP%>">
                                                            <% for(let loiIch of luaChonPhong.loiIchPhongO) {%>
                                                                <p>
                                                                    <%= loiIch %>
                                                                </p>
                                                                <% } %>
                                                        </div>
                                                    </div>

                                                    <div class="sleeps">
                                                        <div class="title">
                                                            Sức chứa
                                                        </div>

                                                        <div class="content">
                                                            <i class="fa-solid fa-user"></i> x <span>
                                                                <%=luaChonPhong.sucChuaNguoiLon%>
                                                            </span> <br>
                                                            <i class="fa-solid fa-child-reaching"></i> x <span>
                                                                <%=luaChonPhong.sucChuaTreEm%>
                                                            </span>

                                                            <input hidden class="sleeps__adult"
                                                                value="<%=luaChonPhong.sucChuaNguoiLon%>">

                                                            <input hidden class="sleeps__children"
                                                                value="<%=luaChonPhong.sucChuaTreEm%>">

                                                        </div>
                                                    </div>

                                                    <div class="pay-cancel">
