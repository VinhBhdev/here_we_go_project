                                                                <b>Thanh toán</b><br>
                                                                <%= luaChonPhong.chinhSachThanhToan%>
                                                        </div>
                                                    </div>

                                                    <div class="quantity">
                                                        <div class="title">
                                                            Số lượng
                                                        </div>

                                                        <div class="content">
                                                            <!-- <div class="f1">
                                                                    
                                                                </div> -->
