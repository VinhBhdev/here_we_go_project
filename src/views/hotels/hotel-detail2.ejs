        <script>
            let xulyTien = (s) => {
                s = "" + s;
                let res = '';
                let cnt = 0;
                for (let i = s.length - 1; i >= 0; i--) {
                    cnt++;
                    res += s[i];
                    if (cnt % 3 == 0 && i != 0) res += ',';
                }
                res = res.split("").reverse().join("");
                return res;
            }
