                            <div class="navbar-nav">
                                <a class="nav-item nav-link" href="#">Tổng quan</a>
                                <a class="nav-item nav-link" href="#hotel-rooms">Phòng nghỉ</a>
                                <a class="nav-item nav-link" href="#hotel-benefit">Tiện nghi</a>
